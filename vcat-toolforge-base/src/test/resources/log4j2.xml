<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="systemOut" target="SYSTEM_OUT">
			<PatternLayout>
				<Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
			</PatternLayout>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="error">
			<AppenderRef ref="systemOut" />
		</Root>
	</Loggers>
</Configuration>
